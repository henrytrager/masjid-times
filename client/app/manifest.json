{
  "manifest_version": 2,
  "name": "Masjid Times",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["chrome.js"]
    }
  },
  "permissions": [
    "geolocation",
    "storage",
    "http://localhost:8888/"
  ],
  "offline_enabled" : true
}